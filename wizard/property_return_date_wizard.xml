<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="return_date_form" model="ir.ui.view">
       <field name="name">return.date.view.form</field>
       <field name="model">property.return.date.wizard</field>
       <field name="arch" type="xml">
           <form string="hold date">
               <group>
                   <field name="custody_id" invisible="1"/>
                   <field name="return_date"/>
               </group>
               <footer>
                   <button name="action_submit_return_date" string="Submit" type="object" class="btn-primary"/>
                   <button name="cancel" string="Cancel" special="cancel"/>
               </footer>
           </form>
       </field>
   </record>
</odoo>